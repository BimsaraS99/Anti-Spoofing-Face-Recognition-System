  <property name="styleSheet">
   <string notr="true">QWidget {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #1a1a2e, stop:1 #16213e
    );
    color: #e0e0e0;
}

/* Main window and central widget */
QMainWindow {
    background: transparent;
}

/* Buttons */
QPushButton {
    background-color: #394867;
    border: 1px solid #4a5a7b;
    border-radius: 4px;
    padding: 5px;
    min-width: 80px;
}

QPushButton:hover {
    background-color: #4a5a7b;
}

QPushButton:pressed {
    background-color: #2c3a4d;
}

/* Line edits */
QLineEdit, QTextEdit, QPlainTextEdit {
    background-color: #252a3d;
    border: 1px solid #4a5a7b;
    border-radius: 3px;
    padding: 5px;
    selection-background-color: #394867;
}

/* Labels */
QLabel {
    color: #e0e0e0;
}

/* Combo boxes */
QComboBox {
    background-color: #252a3d;
    border: 1px solid #4a5a7b;
    border-radius: 3px;
    padding: 5px;
    min-width: 6em;
}

QComboBox::drop-down {
    border: 0px;
}

QComboBox QAbstractItemView {
    background-color: #252a3d;
    selection-background-color: #394867;
}

/* Sliders */
QSlider::groove:horizontal {
    height: 8px;
    background: #252a3d;
    border-radius: 4px;
}

QSlider::handle:horizontal {
    background: #e0e0e0;
    border: 1px solid #4a5a7b;
    width: 16px;
    margin: -4px 0;
    border-radius: 8px;
}

/* Checkboxes and radio buttons */
QCheckBox, QRadioButton {
    spacing: 5px;
}

QCheckBox::indicator, QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #4a5a7b;
}

QCheckBox::indicator {
    background-color: #252a3d;
    border-radius: 3px;
}

QRadioButton::indicator {
    background-color: #252a3d;
    border-radius: 8px;
}

QCheckBox::indicator:checked, QRadioButton::indicator:checked {
    background-color: #394867;
}

/* Group boxes */
QGroupBox {
    border: 1px solid #4a5a7b;
    border-radius: 5px;
    margin-top: 10px;
    padding-top: 15px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 3px;
}

/* Scroll bars */
QScrollBar:vertical {
    border: 1px solid #252a3d;
    background: #252a3d;
    width: 10px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background: #394867;
    min-height: 20px;
    border-radius: 4px;
}

/* Progress bars */
QProgressBar {
    border: 1px solid #4a5a7b;
    border-radius: 3px;
    text-align: center;
}

QProgressBar::chunk {
    background-color: #394867;
}</string>
